[Grid]
LowerLeft = -0.5 -0.5
UpperRight = 2 1.5
Cells = 5 5

[Problem]
Name = test_kovasznay # name passed to the output routines
EnableGravity = false
PrintL2Error = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 0.025

[ Newton ]
EnableShiftCriterion = false
EnableSIMPLEAbsoluteResidualCriterion = true
MaxSteps = 15000
MaxRelativeShift = 1e-5

[Vtk]
WriteFaceData = false

[Algorithm]
AlgorithmType = 4

[LinearSolver2]
schurGMResRestart = 100
schurResidualReduction = 1e-10
schurVerbosity = 0
schurMaxIter = 2000
precVerbosityA = 0
precVerbositySchurAppr = 0
precMaxIter = 2000
precResidReduction = 1e-13

[Underrelaxation]
HundredTimesVelocityUnderrelaxationFactor = 10
HundredTimesPressureUnderrelaxationFactor = 10
