[TimeLoop]
DtInitial = 1e-3 # [s]
TEnd = 0.15 # [s]
HaveMaxTimeStepSize = true
MaxTimeStepSize = 1e-3

[Grid]
UpperRight = 1 1
Cells = 50 50

[Problem]
Name = test_liddrivencavity_re1 # name passed to the output routines
LidVelocity = 1
EnableGravity = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 1

[ Newton ]
EnableShiftCriterion = false
EnableSIMPLEAbsoluteResidualCriterion = true
MaxSteps = 10000
MaxRelativeShift = 1e-8
MaxTimeStepDivisions = 0

[Vtk]
WriteFaceData = false

[LinearSolver2]
schurGMResRestart = 100
schurResidualReduction = 1e-10
schurVerbosity = 0
schurMaxIter = 2000
precVerbosityA = 0
precVerbositySchurAppr = 0
precMaxIter = 2000
precResidReduction = 1e-13
