[Problem]
EnableGravity = false

[SpatialParams]
Permeability = 1 # m^2
AlphaBeaversJoseph = 1.0

[Domain1]
SolverType = Darcy
DiscretizationScheme = Tpfa
Grid.LowerLeft = 0 0
Grid.UpperRight = 1 1
Grid.Cells = 60 80
Problem.Name = domain1
Problem.IsOnNegativeMortarSide = false

[Domain2]
SolverType = Darcy
DiscretizationScheme = Tpfa
Grid.LowerLeft = 0 1
Grid.UpperRight = 1 2
Grid.Cells = 80 80
Problem.Name = domain2
Problem.IsOnNegativeMortarSide = true

[Mortar]
Grid.LowerLeft = 0 1
Grid.UpperRight = 1 1
Grid.Cells = 70
VariableType = Flux

[InterfaceSolver]
Verbosity = 2
ResidualReduction = 1e-10
MaxIterations = 2000
LinearSolverType = GMRes

[Vtk]
OutputName = mortarcoupling
AddVelocity = 1

[L2Error]
IntegrationOrder = 4

[Assembly]
NumericDifference.BaseEpsilon = 1

[0.Component]
LiquidDensity = 1
LiquidKinematicViscosity = 1
